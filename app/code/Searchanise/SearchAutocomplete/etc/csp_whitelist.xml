<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <!-- Searchanise style hosts -->
        <policy id="style-src">
            <values>
                <value id="searchanise_style" type="host">www.searchanise.com</value>
                <value id="searchanise_style_dev" type="host">*.searchserverapi.com</value>
                <value id="searchanise_cdn" type="host">searchanise-ef84.kxcdn.com</value>
                <value id="searchanise_amazon_s3" type="host">s3.amazonaws.com</value>
                <value id="twitter_styles" type="host">*.twitter.com</value>
                <value id="twitter_styles_ton" type="host">ton.twimg.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="searchanise_frame" type="host">www.searchanise.com</value>
                <value id="searchanise_frame_dev" type="host">*.searchserverapi.com</value>
                <value id="searchanise_frame_twitter" type="host">*.twitter.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="searchanise_font" type="host">www.searchanise.com</value>
                <value id="searchanise_font_dev" type="host">*.searchserverapi.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="amplitude_src" type="host">api.amplitude.com</value>
                <value id="stats_doubleclick" type="host">stats.g.doubleclick.net</value>
                <value id="ga_src" type="host">www.google-analytics.com</value>
            </values>
        </policy>
        <policy id="form-action">
            <values>
                <value id="form_searchanise" type="host">www.searchanise.com</value>
                <value id="form_searchanise_dev" type="host">*.searchserverapi.com</value>
                <value id="twitter_" type="host">*.twitter.com</value>
            </values>
        </policy>
        <!-- Searchanise js hosts -->
        <policy id="script-src">
            <values>
                <value id="searchanise_cdn" type="host">searchanise-ef84.kxcdn.com</value>
                <value id="searchanise_amazon_s3" type="host">s3.amazonaws.com</value>
                <value id="searchanise_jquery" type="host">ajax.aspnetcdn.com</value>
                <value id="searchanise_main" type="host">www.searchanise.com</value>
                <value id="searchanise_dev" type="host">*.searchserverapi.com</value>
                <value id="searchanise_static" type="host">*.searchanise.com</value>
                <value id="searchanise_amplitude" type="host">api.amplitude.com</value>
                <value id="searchanise_twitter" type="host">*.twitter.com</value>
                <value id="searchanise_cdn_twitter" type="host">*.twimg.com</value>
            </values>
        </policy>
        <!-- Alows data: images -->
        <policy id="img-src">
            <values>
                <value id="se_data" type="host">data:</value>
                <value id="twitter_image" type="host">*.twitter.com</value>
                <value id="twimg_image" type="host">*.twimg.com</value>
                <value id="google_image" type="host">www.google.com</value>
                <value id="google_image_ru" type="host">www.google.ru</value>
                <value id="se_image" type="host">www.searchanise.com</value>
                <value id="se_image_dev" type="host">*.searchserverapi.com</value>
                <value id="searchanise_amazon_s3" type="host">s3.amazonaws.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
